{% extends "base.html" %}
{% block title %}Pharmaceutical Detail - {{ pharmaceutical }}{% endblock title%}

{% block header %}Pharmaceutical Detail - {{ pharmaceutical }}{% endblock header%}

{% block content %}
<table>
	<thead>
	</thead>
	<tbody>
		<tr><th>Injection</th><td>{{ pharmaceutical.drug }}</td></tr>
		<tr><th>Dose</th><td>{{ pharmaceutical.dose }}</td></tr>
		<tr><th>Recurrance</th><td>{{ pharmaceutical.recurrance }}</td></tr>
		<tr><th>Mode</th><td>{{ pharmaceutical.mode }}</td></tr>
		<tr><th>Vendor</th><td>{{ pharmaceutical.vendor }}</td></tr>
		<tr><th>Notes</th><td>{{ pharmaceutical.notes }}</td></tr>
	</tbody>
</table>
{% endblock content %}
